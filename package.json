{
  "name": "strapi-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Strapi starter with PostgreSQL database, test setup and Docker",
  "license": "MIT",
  "author": {
    "name": "PALO IT"
  },
  "scripts": {
    "build": "strapi build",
    "develop": "strapi develop",
    "docker:dev": "docker-compose -f ./docker-compose.yml --env-file .env up -d",
    "postinstall": "husky",
    "start": "strapi start",
    "strapi": "strapi",
    "test": "jest --runInBand --forceExit",
    "test:reset-database": "docker rm -f strapi-starter-test-database; docker volume rm strapi-starter_strapi-starter-test-data; yarn docker:dev"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@strapi/plugin-cloud": "4.21.1",
    "@strapi/plugin-i18n": "4.21.1",
    "@strapi/plugin-users-permissions": "4.21.1",
    "@strapi/strapi": "4.21.1",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "5.3.4",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "79187c9d-dc09-4cc4-847f-6aff82750b92",
    "telemetryDisabled": true
  }
}
